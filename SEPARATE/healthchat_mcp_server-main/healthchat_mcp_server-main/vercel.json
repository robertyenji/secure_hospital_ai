{
  "version": 2,
  "builds": [
    {
      "src": "vercel_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel_app.py"
    }
  ],
  "env": {
    "SECRET_KEY": "@secret-key",
    "DATABASE_URL": "@database-url",
    "JWT_ALG": "HS256"
  }
}
